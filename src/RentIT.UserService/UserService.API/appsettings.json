{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "PostgresDB": "Host=$DB_HOST; Port=$DB_PORT; Database=$DB_NAME; Username=$DB_USER; Password=$DB_PASSWORD"
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": { "Url": "http://0.0.0.0:8080" },
      "Https": { "Url": "http://0.0.0.0:8081"}
    }
  }
}
